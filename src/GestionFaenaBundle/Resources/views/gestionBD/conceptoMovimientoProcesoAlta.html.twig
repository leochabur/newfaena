{% extends "FOSUserBundle::layout.html.twig" %}
        

{% block fos_user_content %}
	<div class="container">
		<br>
<p class="h5 card-title">Asignar Concepto a Proceso</p>
<hr>
{% for message in app.flashes('sussecc') %}
	<div class="alert alert-success" role="alert">
	  {{ message }}
	</div>
{% endfor %}
		{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
		{{ form_label(form.procesoFaena) }}
		{{ form_widget(form.procesoFaena, { 'attr' : { 'class' : 'col-3' }}) }}
		{{ form_label(form.concepto) }}
		{{ form_widget(form.concepto, { 'attr' : { 'class' : 'col-3' }}) }}
		{{ form_label(form.tipoMovimiento, 'Tipo Movimiento') }}
		{{ form_widget(form.tipoMovimiento, { 'attr' : { 'class' : 'col-3' }}) }}
		<small id="defaultRegisterFormPasswordHelpBlock" class="form-text text-muted mb-4">
        Mantener la tecla CTRL para seleccionar mas de uno
    </small>
		{{ form_widget(form.guardar) }}
		{{ form_end(form) }}
	</div>
{% endblock fos_user_content %}
