{% extends "FOSUserBundle::layout.html.twig" %}
{% set show_nav = false %}
{% block fos_user_content %}
<p class="h5 card-title">
	Fecha Faena: {{ faena }}
</p>
<hr>

<div class="card">
	<div class="card-header">
		<div class="h5">
			Detalle de Ingresos
			<hr>
		</div>

  	</div>
	<div class="card-body">
		<table class="table table-striped table-bordered table-hover table-sm ">
			<thead>
				<tr>
					<th>#</th>
					<th>Fecha Faena</th>
					<th>Origen</th>
					<th>Articulo</th>
					<th>Bultos</th>
				</tr>
			</thead>
			<tbody>
				
				{% for m in body %}
						<tr>
							<td>{{ loop.index }}</td>
							<td>{{ m.fecha }}</td>
							<td>{{ m.origen }}</td>
							<td>{{ m.articulo }}</td>
							<td class="text-right">{{ m.valor }}</td>
						</tr>		
				{% endfor %}

			</tbody>
		</table>
</div>



{% endblock fos_user_content %}

{% block javascripts %}

<script type="text/javascript">


</script>

{% endblock %}
